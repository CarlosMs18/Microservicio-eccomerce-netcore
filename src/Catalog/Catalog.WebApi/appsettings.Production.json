{
  "Catalog": {
    "DatabaseName": "CatalogDB"
  },
  "ConnectionParameters": {
    "server": "microservices-sql-server.database.windows.net,1433",
    "database": "CatalogDB",
    "user": "sqladmin",
    "trust": "false",
    "encrypt": "true"
  },
  "ConnectionPooling": {
    "maxPoolSize": "100",
    "minPoolSize": "5",
    "connectionTimeout": "30",
    "commandTimeout": "45"
  },
  "ServiceParameters": {
    "host": "user-service.prod.svc.cluster.local",
    "port": "5001"
  },
  "RabbitMQParameters": {
    "host": "rabbitmq-service.prod.svc.cluster.local",
    "port": "5672",
    "username": "",
    "password": "",
    "virtualhost": "/"
  },
  "Microservices": {
    "User": {
      "host": "user-service.prod.svc.cluster.local",
      "port": "5001"
    }
  },
  "Ports": {
    "Rest": 80,
    "Grpc": 7205
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    }
  }
}