{
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Grpc": "Warning"
    }
  },
  "UserMicroservice": {
    "TimeoutSeconds": 10
  },
  "HttpClientPolicies": {
    "RetryCount": 3,
    "RetryDelaySec": 2,
    "CircuitBreakerFailures": 5,
    "CircuitBreakerDurationSec": 30
  },
  "Grpc": {
    "EnableDetailedErrors": true,
    "MaxMessageSizeMB": 16
  }
}