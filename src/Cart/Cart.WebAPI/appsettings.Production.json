{
  "Cart": {
    "DatabaseName": "CartDB"
  },
  "ConnectionParameters": {
    "server": "microservices-sql-server.database.windows.net,1433",
    "database": "CartDB",
    "user": "sqladmin",
    "trust": "false",
    "encrypt": "true"
  },
  "ConnectionPooling": {
    "maxPoolSize": "150",
    "minPoolSize": "8",
    "connectionTimeout": "45",
    "commandTimeout": "60"
  },
  "ServiceParameters": {
    "host": "user-service.prod.svc.cluster.local",
    "port": "5001"
  },
  "RabbitMQParameters": {
    "host": "rabbitmq-service.prod.svc.cluster.local",
    "port": "5672",
    "username": "",
    "password": "",
    "virtualhost": "/"
  },
  "Microservices": {
    "User": {
      "host": "user-service.prod.svc.cluster.local",
      "port": "5001"
    },
    "Catalog": {
      "host": "catalog-service.prod.svc.cluster.local",
      "port": "7205"
    }
  },
  "Ports": {
    "Rest": 80
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    }
  }
}